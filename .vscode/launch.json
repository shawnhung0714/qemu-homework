    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb)vadd-vm",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/aarch64-linux-user/qemu-aarch64",
            "args": [
                "-D",
                "logging.log",
                "/home/shawnhung/Downloads/hw-src/vadd-vm"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "TargetsOfBranch",
                    "value": "400798"
                },
                {
                    "name": "EntriesOfBasicBlock",
                    "value": "400700"
                },
                {
                    "name": "ConditionalBranchInfo",
                    "value": "40071c"
                }
            ],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb)encr-vm",
